bitbake -retroarch | grep -A 50 "do_install()"
bitbake -retroarch | grep "do_install()"
bitbake -retroarch
bitbake -retroarch
bitbake -retroarch | grep
bitbake -retroarch | grep "d*"
bitbake-layers show-appends retroarch
bitbake -e retroarch | grep -A 30 "do_install:append()"
bitbake-layers show-appends retroarch
bitbake -e retroarch | grep "do_install"
